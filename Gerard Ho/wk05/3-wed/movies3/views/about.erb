<div>
  <h1><%= @title %></h1>
</div>
<div class="wrapper">
  <div class="left">
    <img src= <%=@image%> alt="">
  </div>
  <div class="right">
    <ul>
      <% @items.each do |content| %>
      <% if (content[0] != "Poster" && content[0] != "imdbID" && content[0] != "Response" && content[0] != "Type") %>
        <li><span><%=content[0]%></span> : <%=content[1]%></li>
        <% end %>
      <% end %>
    </ul>
  </div>
</div>
